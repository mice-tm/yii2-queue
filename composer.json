{
  "name": "mice-tm/yii2-queue",
  "type": "yii2-extension",
  "description": "Package base on yii2-queue with some improvements",
  "keywords": ["queue", "yii2", "delayed queue", "amqp"],
  "license": "MIT",
  "authors": [
    {
      "name": "Iryna Mychkova",
      "email": "imychkova@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "micetm\\queue\\": "src"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "^2.0.12",
    "yiisoft/yii2-queue": "^2.0",
    "php-amqplib/php-amqplib": "~2.7"
  },
  "suggest": {
    "ext-pcntl": "Need for process signals."
  },
  "config": {
    "process-timeout": 1800
  }
}
